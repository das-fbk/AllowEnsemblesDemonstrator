<?xml version="1.0" encoding="UTF-8"?>
<tns:process name="FlexibusEmployee_Core" xmlns:tns="http://das.fbk.eu/Process" xmlns:tns1="http://das.fbk.eu/Annotation"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://das.fbk.eu/Process ../Schemata/Process.xsd ">
 
<tns:abstract name="EMPLOYEE_PlanWorkingDay">
	<tns:goal>
		<tns1:point>
			<tns1:domainProperty dp_name="FlexibusTrip">
				<tns1:state>
				ROUTE_ASSIGNED
				</tns1:state>
			</tns1:domainProperty>
		</tns1:point>
	</tns:goal>
</tns:abstract>

<tns:abstract name="EMPLOYEE_ExecuteRoute">
	<tns:goal>
		<tns1:point>
			<tns1:domainProperty dp_name="FlexibusDriverStatus">
				<tns1:state>
				ROUTE_ENDED
				</tns1:state>
			</tns1:domainProperty>
		</tns1:point>
	</tns:goal>
</tns:abstract>

</tns:process>
