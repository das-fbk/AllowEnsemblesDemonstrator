	<tns:transition>
		<tns:initialState>ST0</tns:initialState>
		<tns:finalState>ST1</tns:finalState>
		<tns:action name="RM_StartRouteRequest" actionType="input" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST1</tns:initialState>
		<tns:finalState>ST2</tns:finalState>
		<tns:action name="RM_StartRouteReply" actionType="output" />
	</tns:transition>

</tns:fragment>
