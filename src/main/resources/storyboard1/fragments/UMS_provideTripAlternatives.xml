<?xml version="1.0" encoding="UTF-8"?>
<tns:fragment id="UMS_provideTripAlternatives" xmlns:tns="http://das.fbk.eu/Fragment"
	xmlns:tns1="http://das.fbk.eu/Annotation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://das.fbk.eu/Fragment ../../../schema/allow-ensembles/Fragment.xsd ">

	<tns:state isInitial="true" name="ST0" />
	<tns:state name="ST1" />
	<tns:state name="ST2" />

	<tns:action name="UMS_TripRequest" actionType="input">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="ProvideTrip">
					<tns1:state>INITIAL</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:effect>
			<tns1:event dpName="ProvideTrip" eventName="tripRequested"></tns1:event>
		</tns:effect>
	</tns:action>

	<tns:action name="UMS_TripAlternatives" actionType="output">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="ProvideTrip">
					<tns1:state>TRIP_REQUEST_LOADED</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:effect>
			<tns1:event dpName="ProvideTrip" eventName="alternativeSent"></tns1:event>
		</tns:effect>
	</tns:action>

	<tns:transition>
		<tns:initialState>ST0</tns:initialState>
		<tns:finalState>ST1</tns:finalState>
		<tns:action name="UMS_TripRequest" actionType="input" />
	</tns:transition>

	<tns:transition>
		<tns:initialState>ST1</tns:initialState>
		<tns:finalState>ST2</tns:finalState>
		<tns:action name="UMS_TripAlternatives" actionType="output" />
	</tns:transition>

	<tns:consumerEntityType>User</tns:consumerEntityType>
</tns:fragment>
