	<tns:action name="RM_StartRouteRequest" actionType="input">
		<tns:precondition>
			<tns1:point>
				<tns1:domainProperty dp_name="HandleRoute">
					<tns1:state>INITIAL</tns1:state>
				</tns1:domainProperty>
			</tns1:point>
		</tns:precondition>
		<tns:effect>
			<tns1:event dpName="HandleRoute" eventName="closeRouteSignal"></tns1:event>
		</tns:effect>
	</tns:action>

	<tns:action name="RM_StartRouteReply" actionType="output">
		<tns:effect>
			<tns1:event dpName="HandleRoute" eventName="startRoute"></tns1:event>
		</tns:effect>
	</tns:action>

